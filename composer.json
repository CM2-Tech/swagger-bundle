{
  "name": "cm2-tech/swagger-bundle",
  "description": "Reusable Symfony bundle for generating OpenAPI (Swagger) JSON output with basic protection",
  "type": "symfony-bundle",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.4",
    "zircote/swagger-php": "^4.7"
  },
  "autoload": {
    "psr-4": {
      "CM2\\SwaggerBundle\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  }
}
